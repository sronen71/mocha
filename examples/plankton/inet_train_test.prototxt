###############################################################
layers {
  name: "fc1_side1"
  type: INNER_PRODUCT
  bottom: "pool2"
  top: "fc1_side1"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  inner_product_param {
    num_output: 1024
    weight_filler {
      type: "gaussian"
      std: 0.02
    }
    bias_filler {
      type: "constant"
      value: 0.1
    }
  }
}

layers {
  name: "fc1_side1_relu"
  type: RELU
  bottom: "fc1_side1"
  top: "fc1_side1"
}
layers {
  name: "fc_side_1_drop"
  type: DROPOUT
  bottom: "fc1_side1"
  top: "fc1_side1"
  dropout_param {
    dropout_ratio: 0.5
  }
}


layers {
  name: "fc2_side1"
  type: INNER_PRODUCT
  bottom: "fc1_side1"
  top: "fc2_side1"
