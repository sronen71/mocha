name: "CaffeNet"
layers {
  name: "data1"
  type: DATA
  top: "data1"
  top: "label"
  data_param {
    source: "examples/plankton/plankton_train_lmdb"
    backend: LMDB
    batch_size: 100
  }
  transform_param {
    rotate: true
    resize: 64
    crop_size: 56
    mirror: true
    randsize: true
    distort: true
  }

  include: { phase: TRAIN }
}

layers {
  name: "data2"
  type: DATA
  top: "data2"
  top: "label2"
  data_param {
    source: "examples/plankton/plankton_train_lmdb"
    backend: LMDB
    batch_size: 100
  }
  transform_param {
    rotate: true
    resize: 64
    crop_size: 56
    mirror: true
    randsize: true
    distort: true
    zoom: 2.0
  }

  include: { phase: TRAIN }
}



layers {
  name: "data1"
  type: DATA
  top: "data1"
  top: "label"
  data_param {
    source: "examples/plankton/plankton_val_lmdb"
 #   source: "examples/plankton/plankton_train_lmdb"
    backend: LMDB
    batch_size: 10
  }
  transform_param {
    rotate: false
    resize: 64
    crop_size: 56
    mirror: false
  }

  include: { phase: TEST }
}


layers {
  name: "data2"
  type: DATA
  top: "data2"
  top: "label2"
  data_param {
    source: "examples/plankton/plankton_val_lmdb"
 #   source: "examples/plankton/plankton_train_lmdb"
    backend: LMDB
    batch_size: 10
  }
  transform_param {
    rotate: false
    resize: 64
    crop_size: 56
    mirror: false
    zoom: 2.0
  }

  include: { phase: TEST }
}




layers {
  name: "conv1"
  type: CONVOLUTION
  bottom: "data1"
  top: "conv1"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  convolution_param {
    num_output: 64
    kernel_size: 5
    stride: 1
    pad: 2
    weight_filler {
     type: "gaussian"
     std: 0.01
    }
    bias_filler {
      type: "constant"
      value : 0.1
    }
  }
} 

layers {
  name: "relu1"
  type: RELU
  bottom: "conv1"
  top: "conv1"
}

layers {
  name: "norm1"
  type: LRN
  bottom: "conv1"
  top: "norm1"
  lrn_param {
    local_size: 5
    alpha: 0.0001
    beta: 0.75
  }
}


layers {
  name: "conv1b"
  type: CONVOLUTION
  bottom: "norm1"
  top: "conv1b"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  convolution_param {
    num_output: 64
    kernel_size: 3
    stride: 1
    pad: 1
    weight_filler {
     type: "gaussian"
     std: 0.02
    }
    bias_filler {
      type: "constant"
      value : 0.1
    }
  }
} 

layers {
  name: "relu1b"
  type: RELU
  bottom: "conv1b"
  top: "conv1b"
}


layers {
  name: "conv1c"
  type: CONVOLUTION
  bottom: "conv1b"
  top: "conv1c"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  convolution_param {
    num_output: 96
    kernel_size: 3
    stride: 1
    pad: 1
    weight_filler {
     type: "gaussian"
     std: 0.02
    }
    bias_filler {
      type: "constant"
      value: 0.1
    }
  }
} 

layers {
  name: "relu1c"
  type: RELU
  bottom: "conv1c"
  top: "conv1c"
}



layers {
 name: "pool1"
 type: POOLING
 bottom: "conv1c"
 top: "pool1"
 pooling_param {
  pool: MAX
  kernel_size: 3
  stride: 2
  }
}

layers {
  name: "conv2"
  type: CONVOLUTION
  bottom: "pool1"
  top: "conv2"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  convolution_param {
    num_output: 128
    kernel_size: 5
    stride: 1
    pad: 2
    weight_filler {
     type: "gaussian"
     std: 0.02
    }
    bias_filler {
      type: "constant"
      value: 0.1
    }
  }
} 

